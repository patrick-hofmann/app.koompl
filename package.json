{
  "name": "nuxt-ui-template-dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test:mcp:inspect": "npx @modelcontextprotocol/inspector --config mcp.config ",
    "test:mcp:inspect:kanban": "npx @modelcontextprotocol/inspector --config mcp.config --server kanban",
    "test:mcp:inspect:agents": "npx @modelcontextprotocol/inspector  --config mcp.config --server agents",
    "test:mcp:inspect:datasafe": "npx @modelcontextprotocol/inspector --config mcp.config --server datasafe",
    "test:mcp:inspect:calendar": "npx @modelcontextprotocol/inspector --config mcp.config --server calendar"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/simple-icons": "^1.2.53",
    "@langchain/anthropic": "^0.3.28",
    "@langchain/openai": "^0.6.13",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@nuxt/content": "^3.7.1",
    "@nuxt/ui": "^4.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@unovis/ts": "^1.6.1",
    "@unovis/vue": "^1.6.1",
    "@vueuse/nuxt": "^13.9.0",
    "ai": "^5.0.76",
    "aws4fetch": "^1.0.20",
    "better-sqlite3": "^12.4.1",
    "date-fns": "^4.1.0",
    "mcp-use": "^0.1.18",
    "nanoid": "^5.1.6",
    "nuxt": "^4.0.3",
    "nuxt-auth-utils": "^0.5.25",
    "nuxt-security": "2.4.0",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.18",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@iconify-json/vscode-icons": "^1.2.31",
    "@nuxt/eslint": "^1.9.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.4.2",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.8"
  },
  "resolutions": {
    "unimport": "4.1.1"
  },
  "packageManager": "pnpm@10.17.1",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
